# Host *
# Host dragomirdanut
Host github.com:dragomirdanut
	HostName github.com
	User git
	# IgnoreUnknown UseKeychain
	AddKeysToAgent yes
	# UseKeychain yes
	PreferredAuthentications publickey
	IdentityFile ~/.ssh/dragomirdanut

